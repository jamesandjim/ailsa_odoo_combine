<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
    <template id="assets_backend" name="Website Studio Assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/website_studio/static/src/js/form_manager.js"></script>
            <script type="text/javascript" src="/website_studio/static/src/js/submenu.js"></script>

            <link href="/website_studio/static/src/less/form_manager.less" rel="stylesheet" type="text/less"/>
        </xpath>
    </template>

    <template id="qunit_suite" name="website_studio_tests" inherit_id="web.qunit_suite">
        <xpath expr="//t[@t-set='head']" position="inside">
            <script type="text/javascript" src="/website_studio/static/tests/form_manager_tests.js"></script>
        </xpath>
    </template>
</data>
</odoo>
